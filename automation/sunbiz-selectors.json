{
  "service": "sunbiz",
  "version": "1.0.0",
  "last_updated": "2026-01-27",
  "selectors": {
    "entity_name": {
      "primary": "input[name='P01']",
      "fallbacks": [
        "input[aria-label*='Entity Name']",
        "input[placeholder*='LLC Name']"
      ]
    },
    "principal_address_street": {
      "primary": "input[name='P02']",
      "fallbacks": ["textarea[name*='principal'], input[aria-label*='Street']"]
    },
    "principal_city": {
      "primary": "input[name='P03']",
      "fallbacks": ["input[aria-label*='City']"]
    },
    "principal_zip": {
      "primary": "input[name='P04']",
      "fallbacks": ["input[aria-label*='ZIP'], input[aria-label*='Postal']"]
    },
    "registered_agent_name": {
      "primary": "input[name='P10']",
      "fallbacks": ["input[aria-label*='Registered Agent Name']"]
    },
    "registered_agent_address": {
      "primary": "input[name='P11']",
      "fallbacks": ["input[aria-label*='Agent Address']"]
    },
    "organizer_name": {
      "primary": "input[name='P20']",
      "fallbacks": ["input[aria-label*='Organizer']"]
    },
    "submit_button": {
      "primary": "input[type='submit'][value*='Submit']",
      "fallbacks": [
        "button:has-text('Submit')",
        "input:has-text('Submit')",
        "button[type='submit']"
      ]
    },
    "confirmation_number": {
      "primary": ".confirmation-number",
      "fallbacks": [
        "#tracking-num",
        "span:has-text('Tracking')",
        "div:has-text('Confirmation')"
      ]
    },
    "payment_prepaid_radio": {
      "primary": "input[value='prepaid'][type='radio']",
      "fallbacks": ["label:has-text('Prepaid Account')"]
    },
    "prepaid_account_number": {
      "primary": "#prepaid-account-number",
      "fallbacks": ["input[name*='account'], input[placeholder*='Account Number']"]
    },
    "prepaid_pin": {
      "primary": "#prepaid-pin",
      "fallbacks": ["input[name*='pin'], input[type='password']"]
    }
  },
  "urls": {
    "llc_filing_form": "https://efile.sunbiz.org/corp_ss_llc_menu.html",
    "corporation_filing": "https://efile.sunbiz.org/corp_ss_corp_menu.html",
    "annual_report": "https://efile.sunbiz.org/annual_report.html"
  },
  "timing": {
    "page_load_timeout": 30000,
    "element_wait_timeout": 10000,
    "submit_retry_delay": 2000,
    "screenshot_delay": 500
  },
  "notes": [
    "Sunbiz uses legacy ASP forms - selectors rarely change",
    "Last major UI update: 2019",
    "Monitor 'News & Alerts' page for changes",
    "Fallback selectors use text/aria-label for resilience"
  ]
}
